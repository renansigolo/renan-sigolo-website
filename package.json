{
  "name": "renan-sigolo-website",
  "version": "2.3.0",
  "private": true,
  "homepage": "https://renansigolo.com",
  "author": "Renan Sigolo <renan.sigolo@gmail.com>",
  "bugs": {
    "url": "https://github.com/renansigolo/renan-sigolo-website/issues",
    "email": "renan.sigolo@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/renansigolo/renan-sigolo-website.git"
  },
  "scripts": {
    "astro": "astro",
    "build": "astro build",
    "clean": "rm -rf package-lock.json node_modules dist",
    "compress": "bash scripts/compress-images.sh",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "dev": "astro dev",
    "fix": "npm run lint:js -- --fix && npm run lint:scss -- --fix",
    "format": "prettier '**/*' --write --ignore-unknown",
    "lint:js": "eslint 'src/**/*.{js,astro}'",
    "lint:scss": "stylelint 'src/**/*.{scss,css,astro}'",
    "lint": "npm run lint:js && npm run lint:scss",
    "postinstall": "scripts/setup-hooks.sh",
    "postversion": "git push && git push --tags",
    "preview": "npm run build && astro preview --host",
    "start": "astro dev",
    "test:watch": "concurrently -k npm:dev npm:cy:open",
    "test": "concurrently -k npm:preview npm:cy:run"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "6.2.1",
    "@fortawesome/free-brands-svg-icons": "6.2.1",
    "@fortawesome/free-regular-svg-icons": "6.2.1",
    "@fortawesome/react-fontawesome": "0.2.0",
    "modern-normalize": "1.1.0"
  },
  "devDependencies": {
    "@astrojs/image": "0.13.0",
    "@astrojs/partytown": "1.0.3",
    "@astrojs/react": "2.0.1",
    "@astrojs/sitemap": "1.0.0",
    "@commitlint/cli": "17.4.2",
    "@commitlint/config-conventional": "17.4.2",
    "@commitlint/prompt-cli": "17.4.2",
    "@typescript-eslint/parser": "5.49.0",
    "astro": "2.0.2",
    "astro-compress": "1.1.28",
    "autoprefixer": "10.4.13",
    "concurrently": "7.6.0",
    "cypress": "12.4.1",
    "eslint": "8.32.0",
    "eslint-plugin-astro": "0.23.0",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.32.1",
    "postcss": "8.4.21",
    "postcss-html": "1.5.0",
    "prettier": "2.8.3",
    "prettier-plugin-astro": "0.8.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sass": "1.57.1",
    "sharp": "0.31.3",
    "stylelint": "14.16.1",
    "stylelint-config-prettier": "9.0.4",
    "stylelint-config-recommended-scss": "8.0.0",
    "stylelint-config-sass-guidelines": "9.0.1"
  },
  "engines": {
    "node": "^16.13.0 || ^18.12.0 || >=19.0.0",
    "npm": ">=8.1.0",
    "yarn": "NO LONGER USED - Please use npm"
  },
  "browserslist": [
    "last 1 Chrome version",
    "last 1 Firefox version",
    "last 2 Edge major versions",
    "last 2 Safari major versions",
    "last 2 iOS major versions",
    "Firefox ESR",
    "not IE 11"
  ]
}
