{
  "name": "renan-sigolo-website",
  "version": "2.3.0",
  "private": true,
  "homepage": "https://renansigolo.com",
  "author": "Renan Sigolo <renan.sigolo@gmail.com>",
  "bugs": {
    "url": "https://github.com/renansigolo/renan-sigolo-website/issues",
    "email": "renan.sigolo@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/renansigolo/renan-sigolo-website.git"
  },
  "scripts": {
    "astro": "astro",
    "build": "astro build",
    "clean": "rm -rf package-lock.json node_modules dist",
    "compress": "bash scripts/compress-images.sh",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "dev": "astro dev",
    "fix": "npm run lint:js -- --fix && npm run lint:scss -- --fix",
    "format": "prettier '**/*' --write --ignore-unknown",
    "lint:js": "eslint 'src/**/*.{js,astro}'",
    "lint:scss": "stylelint 'src/**/*.{scss,css,astro}'",
    "lint": "npm run lint:js && npm run lint:scss",
    "postinstall": "scripts/setup-hooks.sh",
    "postversion": "git push && git push --tags",
    "preview": "npm run build && astro preview --host",
    "start": "astro dev",
    "test:watch": "concurrently -k npm:dev npm:cy:open",
    "test": "concurrently -k npm:preview npm:cy:run"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "6.4.2",
    "@fortawesome/free-brands-svg-icons": "6.4.2",
    "@fortawesome/free-regular-svg-icons": "6.4.2",
    "@fortawesome/react-fontawesome": "0.2.0",
    "modern-normalize": "2.0.0"
  },
  "devDependencies": {
    "@astrojs/image": "0.17.1",
    "@astrojs/partytown": "1.2.3",
    "@astrojs/react": "2.3.2",
    "@astrojs/sitemap": "1.4.0",
    "@commitlint/cli": "17.7.1",
    "@commitlint/config-conventional": "17.7.0",
    "@commitlint/prompt-cli": "17.7.1",
    "@typescript-eslint/parser": "5.62.0",
    "astro": "2.10.15",
    "astro-compress": "1.1.48",
    "autoprefixer": "10.4.15",
    "concurrently": "8.2.1",
    "cypress": "12.17.4",
    "eslint": "8.49.0",
    "eslint-plugin-astro": "0.29.0",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.33.2",
    "postcss": "8.4.29",
    "postcss-html": "1.5.0",
    "prettier": "3.0.3",
    "prettier-plugin-astro": "0.12.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sass": "1.66.1",
    "sharp": "0.32.5",
    "stylelint": "15.10.3",
    "stylelint-config-recommended-scss": "12.0.0",
    "stylelint-config-sass-guidelines": "10.0.0"
  },
  "engines": {
    "node": "^16.13.0 || ^18.12.0 || >=20.3.1",
    "npm": ">=8.1.0",
    "yarn": "NO LONGER USED - Please use npm"
  },
  "browserslist": [
    "last 1 Chrome version",
    "last 1 Firefox version",
    "last 2 Edge major versions",
    "last 2 Safari major versions",
    "last 2 iOS major versions",
    "Firefox ESR",
    "not IE 11"
  ]
}
