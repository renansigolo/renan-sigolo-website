{
  "name": "renan-sigolo-website",
  "version": "2.2.1",
  "private": true,
  "homepage": "https://renansigolo.com",
  "author": "Renan Sigolo <renan.sigolo@gmail.com>",
  "bugs": {
    "url": "https://github.com/renansigolo/renan-sigolo-website/issues",
    "email": "renan.sigolo@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/renansigolo/renan-sigolo-website.git"
  },
  "scripts": {
    "astro": "astro",
    "build": "astro build",
    "clean": "rm -rf package-lock.json node_modules dist",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "dev": "astro dev",
    "fix": "npm run lint:scss -- --fix && npm run lint:js -- --fix",
    "format": "prettier '**/*' --write --ignore-unknown",
    "lint:js": "eslint 'assets/js/**/*.js'",
    "lint:scss": "stylelint 'assets/sass/**/*.scss'",
    "lint": "npm run lint:js && npm run lint:scss",
    "postinstall": "scripts/setup-hooks.sh",
    "postversion": "git push && git push --tags",
    "preview": "astro preview --host",
    "start": "astro dev",
    "test": "npm run build && concurrently -k npm:preview npm:cy:run"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "6.2.0",
    "@fortawesome/free-brands-svg-icons": "6.2.0",
    "@fortawesome/react-fontawesome": "0.2.0",
    "modern-normalize": "1.1.0"
  },
  "devDependencies": {
    "@astrojs/image": "0.9.1",
    "@astrojs/partytown": "1.0.0",
    "@astrojs/react": "1.1.4",
    "@astrojs/sitemap": "1.0.0",
    "@commitlint/cli": "17.1.2",
    "@commitlint/config-conventional": "17.1.0",
    "@typescript-eslint/parser": "5.39.0",
    "astro": "1.4.4",
    "astro-compress": "1.0.10",
    "autoprefixer": "10.4.12",
    "concurrently": "7.4.0",
    "cypress": "10.9.0",
    "eslint": "8.24.0",
    "eslint-plugin-astro": "0.19.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-react": "7.31.8",
    "postcss": "8.4.17",
    "postcss-html": "1.5.0",
    "prettier": "2.7.1",
    "prettier-plugin-astro": "0.5.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sass": "1.55.0",
    "sharp": "0.31.1",
    "stylelint": "^14.13.0",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-recommended-scss": "7.0.0",
    "stylelint-config-sass-guidelines": "9.0.1"
  },
  "engines": {
    "node": "^16.13.0 || ^18.0.0",
    "npm": ">=8.1.0",
    "yarn": "NO LONGER USED - Please use npm"
  },
  "browserslist": [
    "last 1 Chrome version",
    "last 1 Firefox version",
    "last 2 Edge major versions",
    "last 2 Safari major versions",
    "last 2 iOS major versions",
    "Firefox ESR",
    "not IE 11"
  ]
}
